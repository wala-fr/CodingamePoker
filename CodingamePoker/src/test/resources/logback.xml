        <appender-ref ref="CONSOLE"/>
    </logger>
    <logger name="com.codingame.model.object.board.WinningCalculator" level="error" additivity="false">
        <appender-ref ref="CONSOLE"/>
    </logger> -->
    <root level="debug">
        <appender-ref ref="CONSOLE"/>
    </root>

</configuration>
